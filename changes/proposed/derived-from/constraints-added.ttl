@base <http://n2t.net/ark:/99152/> .

@prefix periodo: <p0v#> .

@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

periodo:DefinitionShape
  sh:property
    periodo:DefinitionDerivedFromShape
  .

periodo:DefinitionDerivedFromShape
  rdf:type sh:PropertyShape ;
  sh:path prov:wasDerivedFrom ;
  sh:name "was derived from" ;
  sh:description "Another period definition from which this one was derived" ;
  sh:nodeKind sh:IRI ;
  sh:pattern "^http://n2t.net/ark:/99152/p0[a-z0-9]{9}" ;
  sh:maxCount 1 ;
  sh:message "Problem with derived from" ;
  .
